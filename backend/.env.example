# Server Configuration
PORT=3001
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview

# Expo/EAS Configuration
EXPO_ACCESS_TOKEN=your_expo_access_token
EAS_BUILD_WEBHOOK_SECRET=your_webhook_secret

# Apple App Store Connect
APPLE_ISSUER_ID=your_issuer_id
APPLE_KEY_ID=your_key_id
APPLE_PRIVATE_KEY_PATH=./secrets/AuthKey.p8

# Google Play Console
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./secrets/google-service-account.json

# JWT Secret for internal auth
JWT_SECRET=your_jwt_secret_key

# Sandbox Configuration
SANDBOX_TIMEOUT_MS=30000
MAX_GENERATED_FILES=50
